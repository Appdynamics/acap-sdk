{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["src"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "opts": {
        "destination": "./docs/",          // same as -d ./out/
        "recurse": true,                  // same as -r
        "tutorials": "./tutorials/"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}